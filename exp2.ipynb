    "import cv2\n",
    "input_path = 'hanu.png' \n",
    "output_path = 'output_image.jpg'\n",
    "src = cv2.imread(input_path)\n",
    "if src is not None:\n",
    "    cv2.imwrite(output_path, src)\n",
    "    print(f\"Image converted and saved as {output_path}\")\n",
    "else:\n",
    "    print(\"Error: Image not found or cannot be read.\")"
   ]
